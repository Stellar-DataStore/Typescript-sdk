{
    "name": "stellards_typescript_sdk",
    "version": "1.0.20",
    "description": "Typescript sdk for StellarDS",
    "author": "TMSSoftware",
    "repository": {
        "type": "git",
        "url": "https://github.com/Stellar-DataStore/Typescript-sdk.git"
    },
    "keywords": [
        "fetch",
        "typescript",
        "openapi-client",
        "openapi-generator"
    ],
    "license": "Unlicense",
    "main": "./dist/index.js",
    "type": "commonjs",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./services/*": {
            "require": "./dist/services/*.js",
            "import": "./dist/services/*.js"
        }
    },
    "files": [
        "dist"
    ],
    "typings": "./dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "prepare": "npm run build"
    }
}
